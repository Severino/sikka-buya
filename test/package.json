  "directories": {
    "test": "npm run test:api && npm run test:cypress:run",
    "test:api": "grunt test",
    "test:backend:reload": "nodemon testing-backend.js",
    "test:backend": "node testing-backend.js",
    "test:cypress": "concurrently -k -n \"BACKEND,CYPRESS,VUE\"  -c \"magenta,cyan,blue\" \"npm run test:backend \" \"npm run test:cypress-app\" \"npm run frontend\" ",
    "test:cypress:run": "concurrently -k -n \"BACKEND,CYPRESS,VUE\"  -c \"magenta,cyan,blue\" \"npm run test:backend \" \"npm run test:cypress-run\" \"npm run frontend\" ",
    "test:cypress:backend": "grunt backend",
    "test:cypress:record": "concurrently -k -n \"BACKEND,CYPRESS,VUE\"  -c \"magenta,cyan,blue\" \"npm run test:backend \" \"npm run test:cypress-record\" \"npm run frontend\"  ",
    "test:cypress-record": "wait-on http://localhost:4000 && wait-on http://localhost:8080 && node cypress-run.js",
    "test:cypress-run": "wait-on http://localhost:4000 && wait-on http://localhost:8080 && npx cypress run",
    "test:cypress-app": "wait-on http://localhost:4000 && wait-on http://localhost:8080 && cypress open"
